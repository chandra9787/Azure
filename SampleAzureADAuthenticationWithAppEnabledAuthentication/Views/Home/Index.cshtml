@{
    ViewBag.Title = "Home Page";
}

<h2>@ViewBag.Title</h2>
<table class="table-hover table-condensed table-striped">
    <tr>
        <th>Claim Type</th>
        <th>Claim Value</th>
    </tr>
    @foreach (var claim in Model.Claims)
    {
        <tr>
            <td>@claim.Type</td>
            <td>@claim.Value</td>
        </tr>
    }
</table>